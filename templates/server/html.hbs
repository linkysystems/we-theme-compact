<!DOCTYPE html>
<html lang="{{locale}}">
<head>
  <title>
    {{title}} {{#if title}}|{{/if}}
    {{#if req.we.systemSettings.siteName}}
      {{req.we.systemSettings.siteName}}
    {{else}}
      {{req.we.config.appName}}
    {{/if}}
  </title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
  {{#if _csrf}}<meta name="csrf-token" content="{{_csrf}}" />{{/if}}

  {{#if req.we.systemSettings.iconUrl}}
    <link rel="shortcut icon" href="{{req.we.systemSettings.iconUrl}}">
  {{/if}}

  <!-- Custom fonts for this template -->
  <link href="https://fonts.googleapis.com/css?family=Lato" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Catamaran:100,200,300,400,500,600,700,800,900" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Muli" rel="stylesheet">

  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" integrity="sha256-eZrrJcwDc/3uDhsdt61sL2oOBY362qM3lon1gyExkL0=" crossorigin="anonymous" />

  {{{render-metadata-tags}}}
  {{{render-stylesheet-tags}}}
  {{{render-javascript-tags 'header'}}}
</head>
<body id="page-top" class="body-{{controller}}-{{action}} {{#each req.userRoleNames}}ur-{{this}} {{/each}} {{iframeClass}}">
  <div class="wrapper" style="background-image: url('/public/theme/we-theme-compact/images/default-bg.jpg');">
    <div class="container">
      {{{template 'partials/fb-lib'}}}

      {{we-event event="we-html-body-start"}}
      {{{template 'partials/header'}}}
      {{{layoutHtml}}}
      {{{template 'partials/footer'}}}
      {{we-event event="we-html-body-end"}}
    </div>
  </div>
</body>
</html>